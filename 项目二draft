      "cell_type": "markdown",
      "source": [
        "create  a dataset object using my csv data"
      ],
      "metadata": {
        "id": "zF4_51OsnBkg"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from torch.utils.data import Dataset\n",
        "from transformers import BertTokenizer\n",
        "\n",
        "class QADataset(Dataset):\n",
        "    def __init__(self, questions, contexts, answers, tokenizer, max_length):\n",
        "        self.questions = questions\n",
        "        self.contexts = contexts\n",
        "        self.answers = answers\n",
        "        self.tokenizer = tokenizer\n",
        "        self.max_length = max_length\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.questions)\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        question = str(self.questions[idx])\n",
        "        context = str(self.contexts[idx])\n",
        "        answer = str(self.answers[idx])\n",
        "\n",
        "        # Tokenize question and context\n",
        "        tokenized_input = self.tokenizer.encode_plus(\n",
        "            question, context,\n",
        "            max_length=self.max_length,\n",
        "            padding='max_length',\n",
        "            truncation=True,\n",
        "            return_attention_mask=True,\n",
        "            return_tensors='pt'\n",
        "        )\n",
        "\n",
        "        # Tokenize answer\n",
        "        tokenized_answer = self.tokenizer.encode(\n",
        "            answer,\n",
        "            add_special_tokens=True,\n",
        "            max_length=self.max_length,\n",
        "            truncation=True,\n",
        "            return_tensors='pt'\n",
        "        )\n",
        "\n",
        "        return {\n",
        "            'input_ids': tokenized_input['input_ids'].flatten(),\n",
        "            'attention_mask': tokenized_input['attention_mask'].flatten(),\n",
        "            'labels': tokenized_answer['input_ids'].flatten()\n",
        "        }\n",
        "\n",
        "# 载入数据\n",
        "from google.colab import files\n",
        "\n",
        "# 上传文件\n",
        "uploaded = files.upload()\n",
        "import pandas as pd\n",
        "\n",
        "# 确认上传文件的文件名\n",
        "# 假设您的文件名是 'train项目二.csv'\n",
        "file_name = list(uploaded.keys())[0]  # 修改为您的文件名\n",
        "\n",
        "# 使用pandas读取CSV文件\n",
        "train_data = pd.read_csv(file_name)\n",
        "\n",
        "\n",
        "\n",
        "# 初始化tokenizer\n",
        "tokenizer = BertTokenizer.from_pretrained('bert-base-uncased')\n",
        "\n",
        "# 创建数据集实例\n",
        "max_length = 512  # 设置最大长度\n",
        "dataset = QADataset(\n",
        "    questions=train_data['question'],\n",
        "    contexts=train_data['raw_input'],\n",
        "    answers=train_data['answer'],\n",
        "    tokenizer=tokenizer,\n",
        "    max_length=max_length\n",
        ")\n"
